{
  "name": "Bookmarks-Converter",
  "version": "1.0",
  "description": "get a random bookmark from",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "permissions": [
    "bookmarks",
    "webRequest"
  ],
  "manifest_version": 2,
  "homepage_url": "https://leay.net",
  "author": "hqweay",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}